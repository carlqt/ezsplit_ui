    "@tanstack/react-router": "^1.58.15",
    "@tanstack/router-cli": "^1.58.12",
    "graphql": "^16.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
