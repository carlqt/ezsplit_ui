{
  "name": "ez-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint",
    "preview": "vite preview",
    "codegen": "graphql-codegen --config codegen.ts",
    "db:schema:apply": "docker compose run --rm dev-cli atlas schema apply --env local --auto-approve",
    "db:seed:run": "docker compose run --rm dev-cli go run cmd/seed/main.go",
